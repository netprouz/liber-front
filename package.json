{
  "name": "liber",
  "version": "0.1.0",
  "private": true,
  "author": "Yahyo Temirov",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" ",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --quiet",
    "lint-css": "stylelint \"src/**/*.{js,jsx,ts,tsx}\" --config",
    "prepare": "husky install",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier \"src/**/*.{js,jsx,ts,tsx}\" --check  ",
    "test-all": "yarn check-format && yarn check-lint && yarn check-types && yarn build",
    "lint-format-stage": "lint-staged"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.8.8",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.4.4",
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest",
    "@mui/styled-engine-sc": "^5.8.0",
    "axios": "^0.27.2",
    "i18next": "^21.6.14",
    "i18next-resources-to-backend": "^1.0.0",
    "lodash": "^4.17.21",
    "next": "12.1.0",
    "next-i18next": "^11.0.0",
    "prop-types": "^15.8.1",
    "react": "17.0.2",
    "react-avatar-uploader": "^1.0.3",
    "react-countdown-hook": "^1.1.1",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.27.1",
    "react-i18next": "^11.15.6",
    "react-otp-input": "^2.4.0",
    "react-slick": "^0.28.1",
    "styled-components": "^5.3.5",
    "swiper": "^8.1.4",
    "yup": "^0.32.11"
  },
  "resolutions": {
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write \"src/**/*.{ts,tsx}\" "
    ]
  },
  "devDependencies": {
    "@types/node": "17.0.21",
    "@types/react": "^17.0.30",
    "@types/styled-components": "^5.1.24",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "babel-plugin-styled-components": "^2.0.6",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-prettier": "^1.2.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "4.6.2"
  }
}
